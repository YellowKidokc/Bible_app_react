# Bible MP3 Manager Environment Variables
# Copy this to .env and fill in your actual values

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_R2_ACCESS_KEY=your_r2_access_key_here
CLOUDFLARE_R2_SECRET_KEY=your_r2_secret_key_here

# Database Configuration
POSTGRES_URL=postgresql://root:mariushostingroot@192.168.1.177:2665/QSF

# Optional: Worker Domain (for streaming URL generation)
WORKER_DOMAIN=your-worker-domain.workers.dev

# Optional: Audio Processing Settings
MAX_BATCH_SIZE=100
AUDIO_QUALITY_CHECK=true
ENABLE_DEDUPLICATION=true
